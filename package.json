{
  "name": "birthdayweekly",
  "version": "0.0.1",
  "description": "A little app to remind you about birthdays.",
  "main": "server.js",
  "scripts": {
    "start": "node start.js",
    "test": "echo 'no tests'",
    "build": "gcloud builds submit --tag gcr.io/birthday-weekly/latest",
    "deploy": "npm run deploy:api && npm run deploy:scheduled",
    "deploy:api": "gcloud run deploy --image gcr.io/birthday-weekly/latest --platform managed",
    "deploy:scheduled": "gcloud run deploy birthday-weekly-scheduled --image gcr.io/birthday-weekly/latest --platform managed"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "express": "^4.16.4",
    "googleapis": "^45.0.0",
    "moment": "^2.24.0"
  },
  "engines": {
    "node": "13.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}
